#include<iostream>

using namespace std;

int main()
{
    int n,sum;
    cin>>n>>sum;
    int a[n];
    int ans ;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        ans=0;
  for(int j=i;j<n;j++){
        ans+=a[j];       
if(ans==sum){
    cout<<i<<j;
}
               
  }     
    }
}
