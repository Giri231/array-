#include<iostream>

using namespace std;

int main()
{
    int n,sum=0,sum1=0;
    cin>>n;
    int a[n][n];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>a[i][j];
        }
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            sum=sum+a[i][j];
            sum1=sum1+a[j][i];
        }
    }
    cout<<sum<<" "<<sum1;
}
