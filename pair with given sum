#include<iostream>

using namespace std;
int main()
{
    int n,ans,f,i,j;
    cin>>n;
    cin>>ans;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++ )
        {
          if(a[i]+a[j]==ans)  
         {
               cout<<i<<" "<<j<<endl;
         }
        }
    }
   
      
