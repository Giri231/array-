#include<iostream>
using namespace std;
int main()
{
    int n,ans,sum=0,aman;
    cin>>n>>ans;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
   int max=0;
    for(int i=0;i<n;i++)
    {
       
       sum=sum+a[i];
       if(max<sum)
       {
           max=sum;
       }
       if(sum<0)
       {
           sum=0;
       }
       
    }
    for(int i=0;i<n;i++)
    {
      sum=sum+a[i];
    }
    
    int min=sum-max;
   cout<<min;
    
}
