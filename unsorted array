#include<iostream>
using namespace std;

int main()
{
    int n, pos,val;
    cin>>n>>pos>>val;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(int i=n;i>=pos;i--)
    {
        a[i]=a[i-1];
        
    }
    a[pos-1]=val;
     for(int i=0;i<n;i++)
    {
        cout<<a[i];
    }
    
    
}
