#include<iostream>
using namespace std;

int main()
{
    int n,sum=0,i,j;
    cin>>n;
    int a[n][n];
    cout<<"first";
    for( i=0;i<n;i++)
    {
        for( j=0;j<n;j++)
        {
            cin>>a[i][j];
        }
    }
    for(i=0;i<n;i++)
    {
        
        for( j=0;j<n;j++)
        {
            if(i==j)
            {
                sum=sum+a[i][j];
            }
        }
    }
    
            cout<<sum;
        
        
    
        
}
