#include<iostream>
using namespace std;
int main()
{
    int a[10], a1[10], a2[10];
    int i,j=0,k=0,n;
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
   
    for(int i=0;i<n;i++)
    {
       
        if(a[i]%2==0)
        {
            a1[j]=a[i];
            j++;
        }
        else
        {
        a2[k]=a[i];
        k++;
        }
        
    }
   
    for(int i=0;i<j;i++)
    {
        cout<<a1[i]<<endl;
    }
    for(int i=0;i<k;i++)
    {
        cout<<a2[i]<<endl;
    }
}
