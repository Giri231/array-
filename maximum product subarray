#include<iostream>
using namespace std;

int main()
{
    int n,i,temp;
    cin>>n;
    int a[50],a1[50];
    for(int i=0;i<n;i++)
    {
        
        cin>>a[i];
        
    }
   
   
    int max=a[0];
    
    
   for(int i=0;i<n;i++)
   {
       int pro=1;
       for(int j=i;j<n;j++)
       {
          
       
       pro=pro*a[j];
       }
       if(max<pro)
       {
           max=pro;
       }
       
    
   }
  
  
   
        cout<<max;
    
 
}
