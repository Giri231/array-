#include <iostream>

using namespace std;

int main()
{
   int n,val;
   cin>>n>>val;
   int a[50][50];
   for(int i=0;i<n;i++)
   {
        for(int j=0;j<n;j++)
        {
       cin>>a[i][j];
        }
   }

   for(int i=0;i<n;i++)
   {
         
       for(int j=i+1;j<n;j++)
       {
         if(a[i][j]==val)
         {
             cout<<i<<"and"<<j;
         }
       }
       
   }
 
}
