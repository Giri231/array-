#include<iostream>

using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int pos;
    cin>>pos;
  int  i=0;
  while(i!=pos-1)
            i++;
           while(i<n){
            a[i]=a[i+1];
            i++;
  }
  n--;
  for(i=0;i<n;i++)
        {
		   cout<<a[i];
		}


}
