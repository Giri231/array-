#include<iostream>
using namespace std;
int main()
{
    int m,n,i,j;
    cin>>m>>n;
    int a[m][n] ,b[m][n],sum[m][n];
    cout<<"first ";
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>a[i][j];
        }
    }
    
cout<<"second";
for(int i=0;i<m;i++)
{ 
    cout<<"\n";
    for(int j=0;j<n;j++)
    {
        cin>>b[i][j];
    }
}
cout<<"\n";
cout<<"result";
for(int i=0;i<m;i++)
{
    for(int j=0;j<n;j++)
    {
        sum[i][j]=a[i][j]-b[i][j];
        cout<<sum[i][j]<<" ";
    }
}


}
