#include<iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    
    int num;
    int sum=0;
    int c=n;
    while(n>0)
    {
        num=n%10;
        sum=sum+num*num*num;
        n=n/10;
    }
    if(c==sum)
    {
        cout<<"yes";
    }
    else{
        cout<<"no";
    }
    
}
