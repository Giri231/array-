#include<iostream>


using namespace std;

int main()
{
    int n;
    cin>>n;
    int a[n];
    int temp,j;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    j=n-1;
    for(int i=0;i<n/2;i++)
    {
    
        temp=a[i];
        a[i]=a[j];
        a[j]=temp;
         j--;
  

    }
    for(int i=0;i<n;i++)
    {
        cout<<a[i];
        }
