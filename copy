#include<iostream>

using namespace std;

int main()
{
    int n;
    cin>>n;
    int a1[n];
    int a2[n];
    for(int i=0;i<n;i++)
    {
        cin>>a1[i];
    }
    for(int i=0;i<n;i++)
    {
        a2[i]=a1[i];
    }
    for(int i=0;i<n;i++)
    {
        cout<<a1[i]<<endl;
    }
    for(int i=0;i<n;i++)
    {
        cout<<a2[i];
    }
    return 0;
    
}
